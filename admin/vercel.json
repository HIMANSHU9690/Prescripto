{
    "version": 2,
    "builds": [
      {
        "src": "frontend/build/**",
        "use": "@vercel/static"
      },
      {
        "src": "admin/build/**",
        "use": "@vercel/static"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/admin/(.*)",
        "dest": "/admin/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      }
    ]
  }
  